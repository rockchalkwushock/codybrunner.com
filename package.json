{
	"name": "codybrunner.com",
	"version": "2.0.1",
	"description": "My stretch of pipe in the world wide inter-tubes.",
	"author": {
		"email": "me@codybrunner.com",
		"name": "Cody Brunner",
		"url": "https://codybrunner.com"
	},
	"license": "MIT",
	"type": "module",
	"engines": {
		"node": ">=18.x",
		"pnpm": ">=8.x"
	},
	"scripts": {
		"build": "qwik build",
		"build.client": "vite build",
		"build.preview": "vite build --ssr src/entry.preview.tsx",
		"build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
		"build.types": "tsc --incremental --noEmit",
		"deploy": "wrangler pages publish ./dist",
		"dev": "vite --mode ssr",
		"dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
		"fmt": "prettier --write .",
		"fmt.check": "prettier --check .",
		"lint": "eslint \"src/**/*.ts*\"",
		"preview": "qwik build preview && vite preview --open",
		"serve": "wrangler pages dev ./dist",
		"start": "vite --open --mode ssr",
		"qwik": "qwik"
	},
	"dependencies": {
		"@fontsource/poppins": "5.0.3",
		"@fontsource-variable/playfair-display": "5.0.4",
		"@unpic/core": "0.0.24",
		"@unpic/qwik": "0.0.19"
	},
	"devDependencies": {
		"@builder.io/qwik": "1.1.5",
		"@builder.io/qwik-city": "1.1.5",
		"@tailwindcss/typography": "0.5.9",
		"@types/eslint": "8.40.2",
		"@types/node": "20.3.1",
		"@typescript-eslint/eslint-plugin": "5.59.11",
		"@typescript-eslint/parser": "5.59.11",
		"autoprefixer": "10.4.14",
		"clsx": "1.2.1",
		"eslint": "8.42.0",
		"eslint-plugin-qwik": "1.1.5",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"tailwind-merge": "1.13.1",
		"tailwindcss": "3.3.2",
		"typescript": "5.1.3",
		"undici": "5.22.1",
		"vite": "4.3.9",
		"vite-tsconfig-paths": "4.2.0",
		"wrangler": "^3.0.0"
	}
}
