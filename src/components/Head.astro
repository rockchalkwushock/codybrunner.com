---
import '@styles/global.css'
---

<!-- Global Metadata -->
<meta charset='utf-8' />
<meta name='viewport' content='width=device-width,initial-scale=1' />
<link rel='icon' type='image/svg+xml' href='/favicons/favicon.ico' />
<meta name='generator' content={Astro.generator} />

<!-- Primary Meta Tags -->
<meta name='author' content="Cody Brunner" />
<meta name='description' content="" />
<meta name='title' content="codybrunner.com" />
<title>codybrunner.com</title>

<!-- Open Graph / Facebook -->
<meta property='og:description' content="" />
<meta property='og:image' content={new URL("", Astro.url)} />
<meta property='og:image:alt' content="" />
<meta property='og:image:height' content='800' />
<meta property='og:locale' content="en" />
<meta property='og:image:type' content='image/jpg' />
<meta property='og:image:width' content='600' />
<meta property='og:site_name' content="Cody Brunner" />
<meta property='og:title' content="codybrunner.com" />
<meta property='og:type' content='website' />
<meta property='og:url' content={Astro.url} />

<!-- Twitter -->
<meta property='twitter:card' content='summary_large_image' />
<meta property='twitter:creator' content="@RockChalkDev" />
<meta property='twitter:description' content="" />
<meta property='twitter:image' content={new URL("", Astro.url)} />
<meta property='twitter:site' content="@RockChalkDev" />
<meta property='twitter:title' content="codybrunner.com" />
<meta property='twitter:url' content={Astro.url} />
